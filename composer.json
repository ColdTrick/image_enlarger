	"homepage": "https://www.coldtrick.com",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "images", "content"],
	"license": "GPL-2.0",
	"support": {
		"source": "https://github.com/ColdTrick/image_enlarger",
		"issues": "https://github.com/ColdTrick/image_enlarger/issues"
	},
	"require": {
		"composer/installers": "^1.0.8"
	},
	"conflict": {
		"elgg/elgg": "<5.0"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
